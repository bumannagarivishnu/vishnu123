.siled{
	overflow:hidden;
	height:800px;
	width:1200px;
	margin-left:360px;
	margin-top:20px;
	
}
@-webkit-keyframes slide-animation
{
	0%{left:0px}
	10%{left:0px}
	20%{left:1200px}
	30%{left:12000px}
	40%{left:2400px}
	50%{left:2400px}
	60%{left:1200px}
	70%{left:1200px}
	80%{left:0px}
	90%{left:0px}
	100%{left:0px}
	
	
}
.siled{
	width:3600px;
	height:800px;
	margin:0 0 0 -2400px
	position:relative;
	-webkit-keyframes-name-siled;
	-webkit-keyframes-duration:4s;
	-webkit-keyframes-iteration-count:infinte;
	-webkit-keyframes-direction:right;
	-webkit-keyframes-play-state:running;
}
